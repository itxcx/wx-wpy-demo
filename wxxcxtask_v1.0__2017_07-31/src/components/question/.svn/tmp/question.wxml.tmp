<!--入口模板-->
<template name="question">
  <view class="question">
    <block wx:if="{{question.template === 'choice'}}">
      <template is="choice" data="{{...question, display: display}}" />
    </block>
    <block wx:elif="{{question.template === 'multi-choice'}}">
      <template is="choice" data="{{...question, display: display}}" />
    </block>
    <block wx:elif="{{question.template === 'alter'}}">
      <template is="choice" data="{{...question, display: display}}" />
    </block>
    <block wx:elif="{{question.template === 'fill'}}">
      <template is="fill" data="{{...question, display: display}}" />
    </block>
    <block wx:elif="{{question.template === 'answer'}}">
      <template is="answerQ" data="{{...question, display: display}}" />
    </block>
    <block wx:elif="{{question.template === 'multi'}}">
      <template is="multi" data="{{...question, display: display}}" />
    </block>
    <block wx:elif="{{question.template === 'listen'}}">
      <template is="multi" data="{{...question, display: display}}" />
    </block>
    <block wx:elif="{{question.template === 'cloze'}}">
      <template is="multi" data="{{...question, display: display}}" />
    </block>
    <block wx:elif="{{question.template === 'classify'}}">
      <template is="classify" data="{{...question, display: display}}" />
    </block>
    <block wx:elif="{{question.template === 'connect'}}">
      <template is="connect" data="{{...question, display: display}}" />
    </block>
  </view>
</template>

<!--题干模板-->
<template name="stem">
  <block wx:if="{{display === 'full'}}">
    <view class="stem_box">
      <view class="number">{{index}}</view>
      <view class="stem">
        <template is="wxParse" data="{{wxParseData:stem.nodes}}" />
      </view>
    </view>
  </block>
  <block wx:else>
    <navigator url="../../pages/questionDetail/questionDetail?questionId={{questionId}}&index={{index}}&firstChildIndex={{firstChildIndex}}">
      <view class="stem_box">
        <view class="number">{{index}}</view>
        <view class="stem">
          <template is="wxParse" data="{{wxParseData:stem.nodes}}" />
        </view>
      </view>
    </navigator>
  </block>
</template>

<!--难度模板-->
<template name="difficulty">
  <view class="difficulty">
    <view class="difficulty-tit">
      难度：
    </view>
    <block wx:for="{{[1, 2, 3, 4, 5]}}" wx:for-item="number" wx:key="">
      <block wx:if="{{number <= difficulty}}">
        <view class="full">
          <image class="weui-cell-difficulty" src="/images/difficulty.png" mode="scaleToFill"></image>
        </view>
      </block>
      <block wx:else="{{number <= 5}}">
        <view class="full">
          <image class="weui-cell-difficulty" src="/images/undifficulty.png" mode="scaleToFill"></image>
        </view>
      </block>
    </block>
  </view>
</template>

<!--答案模板-->
<template name="answer">
  <view class="answers">
    <view class="answer-tit">
      答案：
    </view>
    <view class="answer-text">
      <block wx:if="{{template == 'answer'}}">
        <template is="wxParse" data="{{wxParseData:answer.nodes}}"></template>
</block>
<block wx:else>
    {{answer}}
</block>
</view>
</view>
</template>

<!--解析模板-->
<template name="analysis">
  <view class="analysis">
    <view class="analysis-tit">
      解析：
    </view>
    <view class="analysis-text">
      <template is="wxParse" data="{{wxParseData:analysis.nodes}}"></template>
</view>
</view>
</template>

<!--知识点模板-->
<template name="point">
  <view class="point">
    <view class="point-tit">
      知识点：
    </view>
    <view class="point-text">
      {{point}}
    </view>
  </view>
</template>

<!--音频模板-->
<template name="audio">
  <view class="audio">
    <audio src="{{audioSrc}}" controls></audio>
  </view>
</template>

<!--单选/多选/判断模板-->
<template name="choice">
  <view class="{{template}}">
    <template is="stem" data="{{stem: stem, type_id: type_id, index: index, questionId: id, display: display}}" />
    <block wx:if="{{options.length}}">
      <view class="options">
        <view class="option" wx:for="{{options}}" wx:for-item="option" wx:key="">
          <view class="letter">{{option.letter}}.</view>
          <view class="content">
            <template is="wxParse" data="{{wxParseData:option.content.nodes}}"></template>
</view>
</view>
</view>
</block>
<block wx:if="{{display === 'full'}}">
    <view class="analysis_container">
        <block wx:if="{{difficulty}}">
            <template is="difficulty" data="{{difficulty: difficulty}}" />
        </block>
        <block wx:if="{{answer}}">
            <template is="answer" data="{{answer: answer, template: template}}" />
        </block>
        <block wx:if="{{analysis}}">
            <template is="analysis" data="{{analysis: analysis}}" />
        </block>
        <block wx:if="{{point.length}}">
            <template is="point" data="{{point: point}}" />
        </block>
    </view>
</block>
</view>
</template>

<!--填空/问答/翻译/改错/排序模板-->
<template name="fill">
  <view class="{{template}}">
    <template is="stem" data="{{stem: stem, type_id: type_id, index: index, questionId: id, display: display}}" />
    <block wx:if="{{display === 'full'}}">
      <view class="analysis_container">
        <block wx:if="{{difficulty}}">
          <template is="difficulty" data="{{difficulty: difficulty}}" />
        </block>
        <block wx:if="{{answer}}">
          <template is="answer" data="{{answer: answer, template: template}}" />
        </block>
        <block wx:if="{{analysis}}">
          <template is="analysis" data="{{analysis: analysis}}" />
        </block>
        <block wx:if="{{point.length}}">
          <template is="point" data="{{point: point}}" />
        </block>
      </view>
    </block>
  </view>
</template>

<!--/问答/计算单题/解答单题模板-->
<template name="answerQ">
  <view class="{{template}}">
    <template is="stem" data="{{stem: stem, type_id: type_id, index: index, questionId: id, display: display}}" />
    <block wx:if="{{display === 'full'}}">
      <view class="analysis_container">
        <block wx:if="{{difficulty}}">
          <template is="difficulty" data="{{difficulty: difficulty}}" />
        </block>
        <block wx:if="{{answer}}">
          <template is="answer" data="{{answer: answer, template: template}}" />
        </block>
        <block wx:if="{{analysis}}">
          <template is="analysis" data="{{analysis: analysis}}" />
        </block>
        <block wx:if="{{point.length}}">
          <template is="point" data="{{point: point}}" />
        </block>
      </view>
    </block>
  </view>
</template>

<!--计算/解答/材料阅读/阅读理解/听力/听音/完形填空模板-->
<template name="multi">
  <view class="{{template}}">
    <block wx:if="{{stem}}">
      <template is="stem" data="{{stem: stem, type_id: type_id, index: index, questionId: id, display: display, firstChildIndex: children[0].index}}"
      />
    </block>
    <block wx:if="{{template === 'listen'}}">
      <template is="audio" data="{{audioSrc: url}}" />
    </block>
    <block wx:if="{{display === 'full'}}">
      <view class="analysis_container">
        <block wx:if="{{difficulty}}">
          <template is="difficulty" data="{{difficulty: difficulty}}" />
        </block>
      </view>
    </block>
    <block wx:if="{{children.length}}">
      <view class="children">
        <block wx:for-items="{{children}}" wx:for-item="child" wx:key="">
          <block wx:if="{{child.template === 'choice'}}">
            <template is="choice" data="{{...child, difficulty: 0, display: display}}" />
          </block>
          <block wx:if="{{child.template === 'multi-choice'}}">
            <template is="choice" data="{{...child, difficulty: 0, display: display}}" />
          </block>
          <block wx:if="{{child.template === 'alter'}}">
            <template is="choice" data="{{...child, difficulty: 0, display: display}}" />
          </block>
          <block wx:if="{{child.template === 'fill'}}">
            <template is="fill" data="{{...child, difficulty: 0, display: display}}" />
          </block>
          <block wx:if="{{child.template === 'answer'}}">
            <template is="answerQ" data="{{...child, difficulty: 0, display: display}}" />
          </block>
        </block>
      </view>
    </block>
  </view>
</template>

