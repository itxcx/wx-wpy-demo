.question .connect .content_rows {
  padding: 0 0 0 60rpx;
  position: relative;
}

.question .connect .content_rows .connect_screen {
  width: 100%;
  height: 100%;
  position: absolute;
  top: 0;
  left: 0;
}

.question .connect .content_rows .row {
  margin-top: 30rpx;
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  width: 577rpx;
}

.question .connect .content_rows .left,
.question .connect .content_rows .right {
  width: 208rpx;
  height: 148rpx;
  background-color: #e5e5e5;
  border-radius: 17rpx;
}

.question .connect .content_rows .left image,
.question .connect .content_rows .right image {
  width: 208rpx;
  height: 148rpx;
}
