.stem_box {
  font-size: 34rpx;
  padding: 36rpx 30rpx 20rpx;
}

.stem_box .number {
  display: inline-block;
  color: #0052cc;
  width: 60rpx;
  vertical-align: top;
}

.stem_box .stem {
  display: inline-block;
  width: 610rpx;
  vertical-align: top;
}

.stem_box .stem .fill_blank {
  border-bottom: 1px solid #000;
  width: 60rpx;
  display: inline-block;
  vertical-align: middle;
  text-align: center;
}
